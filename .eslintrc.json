{
    "env": {
        "commonjs": false,
        "es6": true,
        "jest": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "script"
    },
    "rules": {
        "prettier/prettier": "error"
    },
    "plugins": [
        "prettier"
    ]
}