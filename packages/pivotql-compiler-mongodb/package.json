{
  "name": "pivotql-compiler-mongodb",
  "version": "0.0.1",
  "description": "Compile PivotQL ASTs to mongodb queries.",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "echo \"nothing to do\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jrmi/pivotql.git"
  },
  "keywords": [
    "query",
    "ast",
    "dsl",
    "unist",
    "mongodb"
  ],
  "author": "Jérémie Pardou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrmi/pivotql/issues"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.2",
    "jest-esm-transformer": "^1.0.0",
    "pivotql-parser-expression": "^0.2.0"
  },
  "jest": {
    "transform": {
      ".js": "jest-esm-transformer"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(pivotql-parser-expression)/)"
    ],
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
